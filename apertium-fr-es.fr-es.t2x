		</call-macro>
		<call-macro n="cv_prep">
			<with-param pos="1"/>
			<with-param pos="2"/>
			<with-param pos="3"/>
		</call-macro>
		<choose>
			<when>
				<test>
					<and>
						<not>
							<equal>
								<clip pos="1" part="tverb"/>
								<lit-tag v="nopart"/>
							</equal>
						</not>
						<equal>
							<clip pos="2" part="tprep"/>
							<lit-tag v="de"/>
						</equal>
						<or>
							<equal>
								<clip pos="3" part="nbr"/>
								<lit-tag v="pl"/>
							</equal>
							<equal>
								<clip pos="3" part="tnom"/>
								<lit-tag v="part"/>
							</equal>
						</or>
					</and>
				</test>
				<out>
					<chunk>
						<clip pos="1" part="lem"/>
						<clip pos="1" part="tags"/>
						<clip pos="1" part="chcontent"/>
					</chunk>
					<b pos="1"/>
					<chunk>
						<clip pos="3" part="lem"/>
						<clip pos="3" part="tags"/>
						<lit-tag v="treu_det"/>
						<clip pos="3" part="chcontent"/>
					</chunk>
				</out>
				<call-macro n="f_bcond">
