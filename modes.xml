        <file name="fra-cat.t1x.bin"/>
      </program>
      <program name="lt-proc -d">
        <file name="fra-cat.autogen.bin"/>
      </program>
    </pipeline>
  </mode>

  <mode name="fra-cat-morph" install="no">
