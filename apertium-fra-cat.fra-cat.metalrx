      <match lemma="Ontario" tags="np.top.m.sg"/>
      <match lemma="est"/>
      <match lemma="ouest"/>
      <match lemma="orient"/>
      <match lemma="occident"/>
    </or>
  </rule>

  <!-- de >> de, a -->
  <rule weight="0.8">
    <match lemma="de" tags="pr"><select lemma="de"/></match>
  </rule>
  <rule weight="1.0">
    <match lemma="obliger" tags="vblex.*"/>
    <repeat from="0" upto="1">
      <or>
        <match tags="adv"/>
        <match tags="cnjadv"/>
      </or>
