<rules>

<!-- **************** DET **************** -->

  <!-- le >> el, remove -->
  <rule weight="0.6">
    <match lemma="le" tags="det.*"><select lemma="el"/></match>
  </rule>
  <rule weight="1.0">
    <or>
      <match lemma="sur" tags="pr"/>
      <match lemma="avec" tags="pr"/>
      <match lemma="sans" tags="pr"/>
      <match lemma="à" tags="pr"/>
      <match lemma="entre" tags="pr"/>
      <match lemma="contre" tags="pr"/>
    </or>
    <match lemma="de" tags="pr"><remove tags="pr"/></match>
    <match lemma="le" tags="det.*"><remove tags="det.*"/></match>
  </rule>

  <!-- beaucoup de >> molt, molt de -->
  <rule weight="0.6">
    <match lemma="beaucoup de" tags="pr"><select lemma="molt"/></match>
  </rule>
  <rule weight="1.0"> <!-- beaucoup d'eux -->
    <match lemma="beaucoup de" tags="pr"><select lemma="molt de"/></match>
    <match tags="prn.*"/>
  </rule>
  <rule weight="1.0"> <!-- beaucoup d'entre eux -->
    <match lemma="beaucoup de" tags="pr"><select lemma="molt de"/></match>
    <match tags="pr"/>
  </rule>

<!-- **************** PREP **************** -->

  <!-- avec >> amb, de -->
  <rule weight="0.6">
    <match lemma="avec" tags="pr"><select lemma="amb"/></match>
  </rule>
  <rule weight="1.0">
    <match lemma="lier# d'amitié" tags="vblex.*"/>
    <match lemma="avec" tags="pr"><select lemma="de"/></match>
  </rule>

  <!-- par >> per, amb, de -->
  <rule weight="0.6">
    <match lemma="par" tags="pr"><select lemma="per"/></match>
  </rule>
  <rule weight="1.0">
    <match lemma="payer" tags="vblex.*"/>
    <repeat from="0" upto="2">
      <or>
        <match tags="prn.enc.*"/>
        <match tags="adv"/>
        <match tags="cnjadv"/>
      </or>
    </repeat>
    <match lemma="par" tags="pr"><select lemma="amb"/></match>
    <or>
      <match lemma="chèque" tags="vblex.*"/>
      <match lemma="carte" tags="vblex.*"/>
    </or>
  </rule>
  <rule weight="1.0">
    <match lemma="faire# engueuler" tags="vblex.*"/>
    <match lemma="par" tags="pr"><select lemma="de"/></match>
  </rule>

  <!-- dans >> en, a -->
  <rule weight="0.6">
    <match lemma="dans" tags="pr"><select lemma="en"/></match>
  </rule>
  <rule weight="1.0">
    <or>
      <match lemma="monter" tags="vblex.*"/>
      <match lemma="descendre" tags="vblex.*"/>
    </or>
    <repeat from="0" upto="2">
      <or>
        <match tags="adv"/>
        <match tags="cnjadv"/>
      </or>
    </repeat>
    <match lemma="dans" tags="pr"><select lemma="a"/></match>
  </rule>
  <rule weight="1.0">
    <match lemma="dans" tags="pr"><select lemma="a"/></match>
    <match lemma="le" tags="det.def.*"/>
    <or>
      <match tags="np.top.f.sg"/>
      <match lemma="Afghanistan" tags="np.top.m.sg"/>
      <match lemma="Angola" tags="np.top.m.sg"/>
      <match lemma="Équateur" tags="np.top.m.sg"/>
      <match lemma="Iran" tags="np.top.m.sg"/>
      <match lemma="Irak" tags="np.top.m.sg"/>
      <match lemma="Iraq" tags="np.top.m.sg"/>
      <match lemma="Israël" tags="np.top.m.sg"/>
      <match lemma="Oman" tags="np.top.m.sg"/>
      <match lemma="Ouzbékistan" tags="np.top.m.sg"/>
      <match lemma="Uruguay" tags="np.top.m.sg"/>
      <match lemma="Alabama" tags="np.top.m.sg"/>
