<rules>

<!-- **************** DET **************** -->

  <!-- le >> el, remove -->
  <rule weight="0.6">
    <match lemma="le" tags="det.*"><select lemma="el"/></match>
  </rule>
  <rule weight="1.0">
    <or>
      <match lemma="sur" tags="pr"/>
      <match lemma="avec" tags="pr"/>
      <match lemma="sans" tags="pr"/>
      <match lemma="à" tags="pr"/>
      <match lemma="entre" tags="pr"/>
      <match lemma="contre" tags="pr"/>
    </or>
    <match lemma="de" tags="pr"><remove tags="pr"/></match>
    <match lemma="le" tags="det.*"><remove tags="det.*"/></match>
  </rule>
  <rule weight="1.0">
    <match lemma="le" tags="det.def.f.sg"><remove tags="det.*"/></match>
    <or>
      <match lemma="mi-janvier" tags="n.*"/>
      <match lemma="mi-février" tags="n.*"/>
