      <match lemma="mi-novembre" tags="n.*"/>
      <match lemma="mi-décembre" tags="n.*"/>
    </or>
  </rule>
  <rule weight="1.0">
    <match lemma="à" tags="pr"/>
    <match lemma="le" tags="det.def.f.sg"><remove tags="det.*"/></match>
    <match lemma="maison" tags="n.*"/>
  </rule>
  <rule weight="1.2">
    <match lemma="à" tags="pr"/>
    <match lemma="le" tags="det.*"><select lemma="el"/></match>
    <match lemma="maison" tags="n.*"/>
    <match lemma="de" tags="pr"/>
  </rule>

  <!-- beaucoup de >> molt, molt de -->
  <rule weight="0.6">
    <match lemma="beaucoup de" tags="pr"><select lemma="molt"/></match>
  </rule>
