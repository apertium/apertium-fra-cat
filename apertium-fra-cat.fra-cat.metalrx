  </rule>

  <!-- encore >> encara, també -->
  <rule weight="0.8">
    <match lemma="encore" tags="adv"><select lemma="encara"/></match>
  </rule>
  <rule weight="1.0">
    <match lemma="ou" tags="cnjcoo"/>
    <match lemma="encore" tags="adv"><select lemma="també"/></match>
  </rule>

  <!-- plus >> més / ja -->
  <rule weight="0.8">
    <match lemma="plus" tags="adv"><select lemma="més"/></match>
  </rule>
  <rule weight="1.0">
    <match lemma="ne" tags="adv"/>
    <repeat from="0" upto="1">
      <match tags="prn.*"/>
    </repeat>
    <or>
      <match tags="vblex.*"/>
      <match tags="vbmod.*"/>
      <match tags="vbser.*"/>
      <match tags="vbhaver.*"/>
    </or>
    <match lemma="plus" tags="adv"><select lemma="ja"/></match>
  </rule>
  <rule weight="1.2">
    <match lemma="plus" tags="adv"><select lemma="més"/></match>
    <match lemma="que"/>
  </rule>

<!-- d'ailleurs >> d'altra banda / d'un altre lloc -->

  <rule weight="0.8">
    <match lemma="d'ailleurs" tags="adv"><select lemma="d'altra banda"/></match>
  </rule>
<!--
  <rule weight="0.8">
    <match lemma="d'ailleurs" tags="adv"><select lemma="d'un altre lloc"/></match>
  </rule>
  <rule weight="1.0">
    <match lemma="D'ailleurs" tags="adv"><select lemma="d'altra banda"/></match>
  </rule>
  <rule weight="1.0">
    <match tags="cm"/>
    <match lemma="d'ailleurs" tags="adv"><select lemma="d'altra banda"/></match>
  </rule>
-->

<!-- d'ailleurs >> d'altra banda / d'un altre lloc -->

  <rule weight="0.8">
    <match lemma="autrement" tags="adv"><select lemma="d'una altra manera"/></match>
  </rule>
  <rule weight="1.0">
    <match lemma="autrement" tags="adv"><select lemma="altrament"/></match>
    <or>
      <match tags="adj.*"/>
      <match tags="vblex.pp.*"/>
    </or>
  </rule>

<!-- **************** NP **************** -->

  <!-- Hercule >> Hèrcules, Hercule -->
  <rule weight="0.8">
    <match lemma="Hercule" tags="np.*"><select lemma="Hèrcules"/></match>
  </rule>
  <rule weight="1.0">
    <match lemma="Hercule" tags="np.*"><select lemma="Hercule"/></match>
    <match tags="np.cog.*"/>
  </rule>

