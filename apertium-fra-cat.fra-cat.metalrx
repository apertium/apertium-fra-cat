    </or>
  </rule>
-->

  <!-- haut >> amunt, alt, altament -->
  <rule weight="0.8">
    <match lemma="haut" tags="adv"><select lemma="amunt"/></match>
  </rule>
  <rule weight="1.0">
    <or>
      <match lemma="parlar" tags="vblex.*"/>
      <match lemma="cridar" tags="vblex.*"/>
    </or>
    <repeat from="0" upto="2">
      <or>
        <match tags="adv"/>
        <match tags="preadv"/>
      </or>
    </repeat>
    <match lemma="haut" tags="adv"><select lemma="alt"/></match>
  </rule>
  <rule weight="1.0">
    <match lemma="n.*"/>
    <match lemma="haut" tags="adv"><select lemma="altament"/></match>
    <match tags="vblex.pp.*"/>
  </rule>

  <!-- assez >> prou, força -->
  <rule weight="0.8">
    <match lemma="assez" tags="adv"><select lemma="prou"/></match>
  </rule>
  <rule weight="1.0">
    <match lemma="assez" tags="adv"><select lemma="força"/></match>
    <or>
      <match tags="adj.*"/>
      <match tags="adv"/>
    </or>
  </rule>
  <rule weight="1.2">
    <match lemma="assez" tags="adv"><select lemma="prou"/></match>
    <or>
      <match tags="adj.*"/>
      <match tags="adv"/>
    </or>
    <match lemma="pour" tags="pr"/>
  </rule>
  <rule weight="1.2">
    <match lemma="pas" tags="adv"/>
    <match lemma="assez" tags="adv"><select lemma="prou"/></match>
  </rule>

  <!-- encore >> encara, també -->
  <rule weight="0.8">
    <match lemma="encore" tags="adv"><select lemma="encara"/></match>
  </rule>
  <rule weight="1.0">
    <match lemma="ou" tags="cnjcoo"/>
    <match lemma="encore" tags="adv"><select lemma="també"/></match>
  </rule>

  <!-- plus >> més / ja -->
  <rule weight="0.8">
    <match lemma="plus" tags="adv"><select lemma="més"/></match>
  </rule>
  <rule weight="1.0">
    <match lemma="ne" tags="adv"/>
    <repeat from="0" upto="1">
      <match tags="prn.*"/>
    </repeat>
    <or>
      <match tags="vblex.*"/>
      <match tags="vbmod.*"/>
      <match tags="vbser.*"/>
      <match tags="vbhaver.*"/>
    </or>
    <match lemma="plus" tags="adv"><select lemma="ja"/></match>
  </rule>
  <rule weight="1.2">
    <match lemma="plus" tags="adv"><select lemma="més"/></match>
    <match lemma="que"/>
  </rule>

<!-- d'ailleurs >> d'altra banda / d'un altre lloc -->

  <rule weight="0.8">
    <match lemma="d'ailleurs" tags="adv"><select lemma="d'altra banda"/></match>
  </rule>
