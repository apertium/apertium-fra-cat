    <match lemma="tout à l'heure" tags="adv"><select lemma="d'aquí una mica"/></match>
  </rule>
  <rule weight="1.0">
    <match lemma="tout à l'heure" tags="adv"><select lemma="d'aquí una mica"/></match>
    <repeat from="0" upto="3">
      <or>
        <match tags="*"/>
        <match tags=""/>
      </or>
    </repeat>
    <or><match tags="vblex.fti.*"/><match tags="vbser.fti.*"/><match tags="vbhaver.fti.*"/><match tags="vbmod.fti.*"/></or>
  </rule>

  <!-- par hasard >> per casualitat, per atzar -->
  <rule weight="0.8">
    <match lemma="par hasard" tags="adv"><select lemma="per casualitat"/></match>
  </rule>

  <!-- pas >> pas, no, ni -->
  <rule weight="0.8">
    <match lemma="pas" tags="adv"><select lemma="pas"/></match>
  </rule>
  <rule weight="1.0">
    <match lemma="però" tags="cnjcoo"/>
    <match lemma="pas" tags="adv"><select lemma="no"/></match>
    <match tags="vblex.pp.*"/>
  </rule>
  <rule weight="1.0">
    <match lemma="pas" tags="adv"><select lemma="no"/></match>
    <or>
      <match lemma="plus" tags="adv"/>
      <match lemma="moins" tags="adv"/>
    </or>
  </rule>
  <rule weight="1.0">
    <match tags="cm"/>
    <match lemma="pas" tags="adv"><select lemma="no"/></match>
  </rule>
  <rule weight="1.2">
    <or>
      <match tags="cm"/>
      <match tags="sent"/>
      <match tags="vblex.*"/>
    </or>
    <match lemma="pas" tags="adv"><select lemma="ni"/></match>
    <match lemma="un"/>
  </rule>
