    <!-- Aquesta macro tria el verb p2 per traduir el prenom p1, però manté p1 si va seguit de cognom (o paraula desconeguda) -->
    <def-macro name="ant_antcog">
      <rule weight="0.6">
         <match lemma="{{p1}}" tags="np.ant.*"><select lemma="{{p2}}"/></match>
      </rule>
      <rule weight="1.0">
         <match lemma="{{p1}}" tags="np.ant.*"><select lemma="{{p1}}"/></match>
         <or>
           <match tags="np.*"/>
           <match tags=""/>
         </or>
      </rule>
    </def-macro>

	<!-- p1 verb fra, p2 verb cat estandard, p3 prep fra, p3 verb cat alternatiu -->
 	<!-- <macro name="verb_prep" p1="chasser" p2="caçar" p3="de" p4="expulsar" /> -->
	<def-macro name="verb_prep">
	  <rule weight="0.6">
		 <match lemma="{{p1}}" tags="vblex.*"><select lemma="{{p2}}"/></match>
	  </rule>
	  <rule weight="1.0">
		 <match lemma="{{p1}}" tags="vblex.*"><select lemma="{{p4}}"/></match>
		 <match lemma="{{p3}}" tags="pr"/>
	  </rule>
	  <rule weight="1.0">
		 <match lemma="{{p1}}" tags="vblex.*"><select lemma="{{p4}}"/></match>
		 <or>
		 	<match tags="adv"/>
		 	<match tags="cnjadv"/>
		 </or>
		 <match lemma="{{p3}}" tags="pr"/>
	  </rule>
	  <rule weight="1.0">
		 <match lemma="{{p1}}" tags="vblex.*"><select lemma="{{p4}}"/></match>
		 <or>
		 	<match tags="adv"/>
		 	<match tags="cnjadv"/>
		 </or>
		 <match tags="adv"/>
		 <match lemma="{{p3}}" tags="pr"/>
	  </rule>
	</def-macro>

	<!-- p1 verb fra, p2 verb cat estandard, p3 i p4 prep fra, p5 verb cat alternatiu -->
 	<!-- <macro name="verb_prep2" p1="foncer" p2="enfosquir" p3="sur" p4="dans" p5="abalançar" /> -->
	<def-macro name="verb_prep2">
	  <rule weight="0.6">
		 <match lemma="{{p1}}" tags="vblex.*"><select lemma="{{p2}}"/></match>
	  </rule>
	  <rule weight="1.0">
		 <match lemma="{{p1}}" tags="vblex.*"><select lemma="{{p5}}"/></match>
		 <or>
		 	<match lemma="{{p3}}" tags="pr"/>
		 	<match lemma="{{p4}}" tags="pr"/>
		 </or>
	  </rule>
	  <rule weight="1.0">
		 <match lemma="{{p1}}" tags="vblex.*"><select lemma="{{p5}}"/></match>
		 <or>
		 	<match tags="adv"/>
		 	<match tags="cnjadv"/>
		 </or>
		 <or>
		 	<match lemma="{{p3}}" tags="pr"/>
		 	<match lemma="{{p4}}" tags="pr"/>
		 </or>
	  </rule>
	  <rule weight="1.0">
		 <match lemma="{{p1}}" tags="vblex.*"><select lemma="{{p5}}"/></match>
		 <or>
		 	<match tags="adv"/>
		 	<match tags="cnjadv"/>
		 </or>
		 <match tags="adv"/>
		 <or>
		 	<match lemma="{{p3}}" tags="pr"/>
		 	<match lemma="{{p4}}" tags="pr"/>
		 </or>
	  </rule>
	</def-macro>

    <!-- Aquesta macro tria el verb p2 si p1 és intransitiu i p3 si p1 és transitiu -->
    <!-- p1 = verb fra, p2 = verb cat intransitiu, p3 = verb cat transitiu -->
    <!-- la manera de veure que el verb és transitiu és molt barroera -->
    <def-macro name="verb_intrans_trans">
      <rule weight="0.8">
        <match lemma="{{p1}}" tags="vblex.*"><select lemma="{{p2}}"/></match>
      </rule>
      <rule weight="1.0">
        <match lemma="{{p1}}" tags="vblex.*"><select lemma="{{p3}}"/></match>
        <or>
          <match tags="det.*"/>
          <match tags="np.*"/>
        </or>
      </rule>
      <rule weight="1.0">
        <match lemma="{{p1}}" tags="vblex.*"><select lemma="{{p3}}"/></match>
        <or>
          <match tags="num.*"/>
          <match tags="num"/>
        </or>
        <match tags="n.*"/>
      </rule>
      <rule weight="1.0">
        <match lemma="{{p1}}" tags="vblex.*"><select lemma="{{p3}}"/></match>
        <or>
          <match tags="num.*"/>
          <match tags="num"/>
        </or>
        <match tags="adj.*"/>
        <match tags="n.*"/>
      </rule>
      <rule weight="1.0">
        <match lemma="le" tags="prn.pro.*"/>
        <match lemma="{{p1}}" tags="vblex.*"><select lemma="{{p3}}"/></match>
      </rule>
      <rule weight="1.0">
        <or>
          <match tags="n.*"/>
          <match tags="adj.*"/>
          <match tags="vblex.pp.*"/>
        </or>
        <match lemma="{{p1}}" tags="vblex.pp.*"><select lemma="{{p3}}"/></match>
      </rule>
    </def-macro>

    <!-- Aquesta macro tria el verb p2 si p1 no és pronominal i p3 si p1 és pronominal -->
    <!-- p1 = verb fra, p2 = verb cat no pron, p3 = verb cat pron -->
    <def-macro name="verb_nopron_pron">
      <rule weight="0.6">
        <match lemma="{{p1}}" tags="vblex.*"><select lemma="{{p2}}"/></match>
      </rule>
      <rule weight="1.0">
        <match tags="prn.pro.p1.*.sg"/>
        <match lemma="{{p1}}" tags="vblex.*.p1.sg"><select lemma="{{p3}}"/></match>
      </rule>
      <rule weight="1.0">
        <match lemma="{{p1}}" tags="vblex.*.p1.sg"><select lemma="{{p3}}"/></match>
        <match tags="prn.enc.p1.*.sg"/>
      </rule>
      <rule weight="1.0">
        <match tags="prn.pro.p2.*.sg"/>
        <match lemma="{{p1}}" tags="vblex.*.p2.sg"><select lemma="{{p3}}"/></match>
      </rule>
      <rule weight="1.0">
        <match lemma="{{p1}}" tags="vblex.*.p2.sg"><select lemma="{{p3}}"/></match>
        <match tags="prn.enc.p2.*.sg"/>
      </rule>
      <rule weight="1.0">
        <match tags="prn.pro.ref.p3.*.sp"/>
        <or>
          <match lemma="{{p1}}" tags="vblex.*.p3.sg"><select lemma="{{p3}}"/></match>
          <match lemma="{{p1}}" tags="vblex.*.p3.pl"><select lemma="{{p3}}"/></match>
          <match lemma="{{p1}}" tags="vblex.inf"><select lemma="{{p3}}"/></match>
          <match lemma="{{p1}}" tags="vblex.ger"><select lemma="{{p3}}"/></match>
        </or>
      </rule>
      <rule weight="1.0">
        <or>
          <match lemma="{{p1}}" tags="vblex.*.p3.sg"><select lemma="{{p3}}"/></match>
          <match lemma="{{p1}}" tags="vblex.*.p3.pl"><select lemma="{{p3}}"/></match>
          <match lemma="{{p1}}" tags="vblex.inf"><select lemma="{{p3}}"/></match>
          <match lemma="{{p1}}" tags="vblex.ger"><select lemma="{{p3}}"/></match>
        </or>
        <match tags="prn.enc.ref.p3.*.sp"/>
      </rule>
      <rule weight="1.0">
        <match tags="prn.pro.p1.*.pl"/>
        <match lemma="{{p1}}" tags="vblex.*.p1.pl"><select lemma="{{p3}}"/></match>
      </rule>
      <rule weight="1.0">
        <match lemma="{{p1}}" tags="vblex.*.p1.pl"><select lemma="{{p3}}"/></match>
        <match tags="prn.enc.p1.*.pl"/>
      </rule>
      <rule weight="1.0">
        <match tags="prn.pro.p2.*.pl"/>
        <match lemma="{{p1}}" tags="vblex.*.p2.pl"><select lemma="{{p3}}"/></match>
      </rule>
      <rule weight="1.0">
        <match lemma="{{p1}}" tags="vblex.*.p2.pl"><select lemma="{{p3}}"/></match>
        <match tags="prn.enc.p2.*.pl"/>
      </rule>

      <rule weight="1.0">
        <match tags="prn.pro.p1.*.sg"/>
        <match lemma="être" tags="vbser.*.p1.sg"/>
        <match lemma="{{p1}}" tags="vblex.pp.*"><select lemma="{{p3}}"/></match>
      </rule>
      <rule weight="1.0">
        <match tags="prn.pro.p2.*.sg"/>
        <match lemma="être" tags="vbser.*.p2.sg"/>
        <match lemma="{{p1}}" tags="vblex.pp.*"><select lemma="{{p3}}"/></match>
      </rule>
      <rule weight="1.0">
        <match tags="prn.pro.p1.*.pl"/>
        <match lemma="être" tags="vbser.*.p1.pl"/>
        <match lemma="{{p1}}" tags="vblex.pp.*"><select lemma="{{p3}}"/></match>
      </rule>
      <rule weight="1.0">
        <match tags="prn.pro.p2.*.pl"/>
        <match lemma="être" tags="vbser.*.p2.pl"/>
        <match lemma="{{p1}}" tags="vblex.pp.*"><select lemma="{{p3}}"/></match>
      </rule>
      <rule weight="1.0">
        <match tags="prn.pro.ref.p3.*.sp"/>
        <or>
          <match lemma="être" tags="vbser.*.p3.sg"/>
          <match lemma="être" tags="vbser.*.p3.pl"/>
        </or>
        <match lemma="{{p1}}" tags="vblex.pp.*"><select lemma="{{p3}}"/></match>
      </rule>
    </def-macro>

    <!-- Aquesta macro tria, per defecte, el nom p2 per traduir p1 (mf) i p3 si p1 està en un context d'un femení -->
    <!-- p1 = nom fra (mf), p2 = nom cat (típicament m), p3 = nom cat (típicament f) -->
    <def-macro name="nom_m_f">
      <rule weight="0.8">
        <match lemma="{{p1}}" tags="n.*"><select lemma="{{p2}}"/></match>
      </rule>
      <rule weight="1.0">
        <or>
          <match tags="det.*.f.*"/>
          <match tags="adj.f.*"/>
        </or>
        <repeat from="0" upto="2">
          <match tags="adj.*"/>
        </repeat>
        <match lemma="{{p1}}" tags="n.*"><select lemma="{{p3}}"/></match>
      </rule>
      <rule weight="1.0">
        <match lemma="{{p1}}" tags="n.*"><select lemma="{{p3}}"/></match>
        <repeat from="0" upto="3">
          <match tags="adj.mf.*"/>
        </repeat>
        <match tags="adj.f.*"/>
      </rule>
    </def-macro>
