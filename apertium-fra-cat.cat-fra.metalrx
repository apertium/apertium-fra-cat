<rules>

<!-- DET -->

  <!-- uns >> des, quelques -->
  <rule weight="0.8">
    <match lemma="un" tags="det.*"><select lemma="un"/></match>
  </rule>
  <rule weight="1.0">
    <or>
      <seq n="pr_de"/>
    </or>
    <or>
      <match lemma="un" tags="det.ind.m.pl"><select lemma="quelque"/></match>
      <match lemma="un" tags="det.ind.f.pl"><select lemma="quelque"/></match>
    </or>
  </rule>
  <rule weight="1.2"> <!-- per solucionar un bug -->
    <match lemma="a" tags="pr"/>
    <match lemma="un" tags="det.*"><select lemma="un"/></match>
  </rule>

<!-- PRN -->

  <!-- aquell >> celui-là, celui -->
  <rule weight="0.8">
    <match lemma="aquell" tags="prn.*"><select lemma="celui-là"/></match>
  </rule>
  <rule weight="1.0">
    <match lemma="aquell" tags="prn.*"><select lemma="celui"/></match>
    <or>
      <match tags="rel.*"/>
      <match tags="pr"/>
    </or>
  </rule>
  <rule weight="1.0">
    <match lemma="aquell" tags="prn.*"><select lemma="celui"/></match>
    <repeat from="0" upto="2">
      <match tags="adv"/>
    </repeat>
    <match tags="adj.*"/>
  </rule>
  <rule weight="1.0">
    <match lemma="aquell" tags="prn.*"><select lemma="celui"/></match>
    <match tags="det.*"/>
    <match tags="n.*"/>
    <repeat from="0" upto="1">
      <match tags="adj.*"/>
    </repeat>
    <match lemma="de" tags="pr"/>
    <match lemma="el qual" tags="rel.*"/>
  </rule>
  <rule weight="1.0">
    <match lemma="aquell" tags="prn.*"><select lemma="celui"/></match>
    <match tags="det.*"/>
    <repeat from="0" upto="1">
      <match tags="adv"/>
    </repeat>
    <match tags="adj.*"/>
    <match tags="n.*"/>
    <match lemma="de" tags="pr"/>
    <match lemma="el qual" tags="rel.*"/>
  </rule>

  <!-- el >> le, lui, leur (per a corregir anàlisis errònies) -->
  <rule weight="0.8">
    <match lemma="el" tags="prn.pro.*"><select lemma="le"/></match>
  </rule>
  <rule weight="1.0">
    <or>
      <match lemma="el" tags="prn.pro.*.sg"><select lemma="lui"/></match>
      <match lemma="el" tags="prn.pro.*.pl"><select lemma="leur"/></match>
    </or>
    <or>
      <match lemma="dir" tags="vblex.*"/>
      <match lemma="prometre" tags="vblex.*"/>
      <match lemma="jurar" tags="vblex.*"/>
      <match lemma="afirmar" tags="vblex.*"/>
      <match lemma="negar" tags="vblex.*"/>
      <match lemma="permetre" tags="vblex.*"/>
      <match lemma="deixar" tags="vblex.*"/>
    </or>
  </rule>
  <rule weight="1.0">
    <or>
      <match lemma="el" tags="prn.pro.*.sg"><select lemma="lui"/></match>
      <match lemma="el" tags="prn.pro.*.pl"><select lemma="leur"/></match>
    </or>
    <or>
      <match tags="vbhaver.*.p3.sg"/>
      <match tags="vbhaver.*.p3.pl"/>
    </or>
    <or>
      <match lemma="dir" tags="vblex.pp.*"/>
      <match lemma="prometre" tags="vblex.pp.*"/>
      <match lemma="jurar" tags="vblex.pp.*"/>
      <match lemma="afirmar" tags="vblex.pp.*"/>
      <match lemma="negar" tags="vblex.pp.*"/>
      <match lemma="permetre" tags="vblex.pp.*"/>
      <match lemma="deixar" tags="vblex.pp.*"/>
    </or>
  </rule>
  <rule weight="1.0">
    <or>
      <match lemma="el" tags="prn.pro.*.sg"><select lemma="lui"/></match>
      <match lemma="el" tags="prn.pro.*.pl"><select lemma="leur"/></match>
    </or>
    <or>
      <match lemma="anar" tags="vblex.pri.p3.sg"/>
      <match lemma="anar" tags="vblex.pri.p3.pl"/>
    </or>
    <or>
      <match lemma="dir" tags="vblex.inf"/>
      <match lemma="prometre" tags="vblex.inf"/>
      <match lemma="jurar" tags="vblex.inf"/>
      <match lemma="afirmar" tags="vblex.inf"/>
      <match lemma="negar" tags="vblex.inf"/>
      <match lemma="permetre" tags="vblex.inf"/>
      <match lemma="deixar" tags="vblex.inf"/>
    </or>
  </rule>

  <!-- si mateix >> soi-même, lui-même -->
  <rule weight="0.8">
    <match lemma="si mateix" tags="prn.*"><select lemma="soi-même"/></match>
  </rule>

  <!-- què >> qu'est-ce que, que, quoi -->
  <rule weight="0.8">
    <match lemma="què" tags="prn.itg.*"><select lemma="qu'est-ce que"/></match>
  </rule>
  <rule weight="1.0">
    <match lemma="què" tags="prn.itg.*"><select lemma="quoi"/></match>
    <match tags="sent"/>
  </rule>

  <!-- es >> se, on -->
  <rule weight="0.8">
    <match lemma="es" tags="prn.*"><select lemma="se"/></match>
  </rule>
  <rule weight="1.0">
    <match lemma="es" tags="prn.pro.*"><select lemma="on"/></match>
    <match tags="vblex.*.p3.sg"/>
    <repeat from="0" upto="1">
      <match tags="adv"/>
    </repeat>
    <match lemma="que" tags="cnjsub"/>
  </rule>
  <rule weight="1.0">
    <match lemma="es" tags="prn.pro.*"><select lemma="on"/></match>
    <match tags="vbhaver.*.p3.sg"/>
    <match tags="vblex.pp.*"/>
    <repeat from="0" upto="1">
      <match tags="adv"/>
    </repeat>
    <match lemma="que" tags="cnjsub"/>
  </rule>
  <rule weight="1.0">
    <match lemma="es" tags="prn.pro.*"><select lemma="on"/></match>
    <or>
      <match lemma="anar" tags="vblex.pri.p3.sg"/>
      <match lemma="anar" tags="vblex.pri.p3.pl"/>
    </or>
    <match tags="vblex.inf"/>
    <repeat from="0" upto="1">
      <match tags="adv"/>
    </repeat>
    <match lemma="que" tags="cnjsub"/>
  </rule>
  <rule weight="1.0">
    <or>
      <match lemma="anar" tags="vblex.pri.p3.sg"/>
      <match lemma="anar" tags="vblex.pri.p3.pl"/>
    </or>
    <match tags="vblex.inf"/>
    <match lemma="es" tags="prn.enc.*"><select lemma="on"/></match>
    <repeat from="0" upto="1">
      <match tags="adv"/>
    </repeat>
    <match lemma="que" tags="cnjsub"/>
  </rule>
  <rule weight="1.0">
    <repeat from="0" upto="1">
      <match lemma="que" tags="rel.an.*"><select lemma="que"/></match>
    </repeat>
    <match lemma="es" tags="prn.*"><select lemma="on"/></match>
    <or>
      <match lemma="comptar" tags="vblex.*"/>
      <match lemma="comptar# amb" tags="vblex.*"/>
      <match lemma="necessitar" tags="vblex.*"/>
    </or>
  </rule>
  <rule weight="1.0">
    <repeat from="0" upto="1">
      <match lemma="que" tags="rel.an.*"><select lemma="que"/></match>
    </repeat>
    <match lemma="es" tags="prn.*"><select lemma="on"/></match>
    <or>
      <match tags="vbhaver.*.p3.sg"/>
      <match tags="vbhaver.*.p3.pl"/>
    </or>
    <or>
