    </repeat>
    <or>
      <match tags="np.top.*"/>
      <match tags="np.loc"/>
      <match lemma="Hornos"/>
    </or>
  </rule>
  <rule weight="1.0">
    <match lemma="cap" tags="n.*"><select lemma="cap"/></match>
    <or>
      <match lemma="nord"/>
      <match lemma="sud"/>
      <match lemma="est"/>
      <match lemma="oest"/>
      <match lemma="septentrional"/>
      <match lemma="meridional"/>
      <match lemma="oriental"/>
      <match lemma="occidental"/>
    </or>
  </rule>
  <rule weight="1.0">
    <match lemma="cap" tags="n.*"><select lemma="patron"/></match>
    <match lemma="de" tags="pr"/>
    <or><match tags="np.al.*"/><match tags="np.al"/></or>
  </rule>
  <rule weight="1.0">
    <match lemma="cap" tags="n.*"><select lemma="chef-lieu"/></match>
    <match lemma="de" tags="pr"/>
    <repeat from="0" upto="2">
