<?xml version="1.0" encoding="UTF-8"?>
<metalrx>

  <def-macros>
	<def-macro name="noms_propis">
	  <rule weight="0.6">
		 <match lemma="{{p1}}" tags="np.*"><select lemma="{{p1}}"/></match>
	  </rule>
	  <rule weight="1.0">
		 <match lemma="{{p1}}" tags="np.*"><select lemma="{{p2}}"/></match>
		 <match lemma="de" tags="pr"/>
		 <or><match tags="np.loc"/><match tags="np.top.*"/></or>
	  </rule>
	  <rule weight="1.0">
		 <match lemma="{{p1}}" tags="np.*"><select lemma="{{p2}}"/></match>
		 <match tags="det.*"/>
		 <match tags="adj.*"/>
	  </rule>
	  <rule weight="1.0">
		 <match lemma="{{p1}}" tags="np.*"><select lemma="{{p2}}"/></match>
		 <or>
		   <match tags="num.*"/>
		   <match lemma="I" tags="adj.*"/>
		   <match lemma="II" tags="adj.*"/>
		   <match lemma="III" tags="adj.*"/>
		   <match lemma="IV" tags="adj.*"/>
		   <match lemma="V" tags="adj.*"/>
		   <match lemma="VI" tags="adj.*"/>
		   <match lemma="VII" tags="adj.*"/>
		   <match lemma="VIII" tags="adj.*"/>
		   <match lemma="IX" tags="adj.*"/>
		   <match lemma="X" tags="adj.*"/>
		   <match lemma="XI" tags="adj.*"/>
		   <match lemma="XII" tags="adj.*"/>
		   <match lemma="XIII" tags="adj.*"/>
		   <match lemma="XIV" tags="adj.*"/>
		   <match lemma="XV" tags="adj.*"/>
		   <match lemma="XVI" tags="adj.*"/>
		   <match lemma="XVII" tags="adj.*"/>
		   <match lemma="XVIII" tags="adj.*"/>
		   <match lemma="XIX" tags="adj.*"/>
		   <match lemma="XX" tags="adj.*"/>
		   <match lemma="XXI" tags="adj.*"/>
		   <match lemma="XXII" tags="adj.*"/>
		   <match lemma="XXIII" tags="adj.*"/>
		   <match lemma="XXIV" tags="adj.*"/>
		   <match lemma="XXV" tags="adj.*"/>
		   <match lemma="XXVI" tags="adj.*"/>
		   <match lemma="XXVII" tags="adj.*"/>
		   <match lemma="XXVIII" tags="adj.*"/>
		   <match lemma="XXIX" tags="adj.*"/>
		   <match lemma="XXX" tags="adj.*"/>
		 </or>
	  </rule>
	  <rule weight="1.0">
		 <or>
		   <match lemma="papa" tags="n.*"/>
		   <match lemma="bisbe" tags="n.*"/>
		   <match lemma="arquebisbe" tags="n.*"/>
		   <match lemma="sant" tags="n.*"/>
		   <match lemma="sant" tags="adj.*"/>
		   <match lemma="apòstol" tags="n.*"/>
		   <match lemma="emperador" tags="n.*"/>
		   <match lemma="rei" tags="n.*"/>
		   <match lemma="tsar" tags="n.*"/>
		   <match lemma="virrei" tags="n.*"/>
		   <match lemma="príncep" tags="n.*"/>
		   <match lemma="infant" tags="n.*"/>
		   <match lemma="delfí" tags="n.*"/>
		   <match lemma="comte" tags="n.*"/>
		   <match lemma="vescomte" tags="n.*"/>
		   <match lemma="duc" tags="n.*"/>
		   <match lemma="arxiduc" tags="n.*"/>
		   <match lemma="marquès" tags="n.*"/>
		   <match lemma="baró" tags="n.*"/>
		 </or>
		 <match lemma="{{p1}}" tags="np.*"><select lemma="{{p2}}"/></match>
	  </rule>
	  <rule weight="1.0">
		 <or>
		   <match lemma="papat" tags="n.*"/>
		   <match lemma="pontificat" tags="n.*"/>
		   <match lemma="imperi" tags="n.*"/>
		   <match lemma="regne" tags="n.*"/>
		   <match lemma="regnat" tags="n.*"/>
		   <match lemma="regència" tags="n.*"/>
		   <match lemma="viregnat" tags="n.*"/>
		 </or>
		 <match lemma="de" tags="pr"/>
		 <match lemma="{{p1}}" tags="np.*"><select lemma="{{p2}}"/></match>
	  </rule>
	</def-macro>
  </def-macros>

<rules>

<!-- REL -->

  <!-- 
    que >> que, qui
  -->
  <rule weight="0.8">
    <match lemma="que" tags="rel.an.*"><select lemma="que"/></match>
  </rule>
  <rule weight="1.0">
    <match lemma="que" tags="rel.an.*"><select lemma="qui"/></match>
    <match tags="vblex.*.p3.*"/>
    <or>
      <match tags="det.*"/>
      <match tags="n.*"/>
      <match tags="adj.*"/>
    </or>
  </rule>
  <rule weight="1.0">
    <match lemma="que" tags="rel.an.*"><select lemma="qui"/></match>
    <match lemma="li" tags="prn.*"/>
    <match tags="vblex.*.p3.*"/>
  </rule>
  <rule weight="1.0">
    <match lemma="que" tags="rel.an.*"><select lemma="qui"/></match>
    <match tags="adv"/>
    <match tags="vblex.*.p3.*"/>
    <or>
      <match tags="det.*"/>
      <match tags="n.*"/>
      <match tags="adj.*"/>
    </or>
  </rule>
  <rule weight="1.0">
    <match lemma="que" tags="rel.an.*"><select lemma="qui"/></match>
    <match tags="adv"/>
    <match lemma="li" tags="prn.*"/>
    <match tags="vblex.*.p3.*"/>
  </rule>

  <!-- 
    la qual cosa >> ce qui, lequel
  -->
  <rule weight="0.8">
    <match lemma="la qual cosa" tags="rel.an.*"><select lemma="ce qui"/></match>
  </rule>
  <rule weight="0.8">
    <match tags="pr"/>
    <match lemma="la qual cosa" tags="rel.an.*"><select lemma="lequel"/></match>
  </rule>

<!-- PR -->

  <!-- 
    per >> par, pour
  -->
  <rule weight="0.8">
    <match lemma="per" tags="pr"><select lemma="par"/></match>
  </rule>
  <rule weight="1.0">
    <match lemma="per" tags="pr"><select lemma="pour"/></match>
    <match tags="*.inf"/>
  </rule>
  <rule weight="1.2">
    <match lemma="felicitar" tags="vblex.*"/>
    <match lemma="per" tags="pr"><select lemma="pour"/></match>
  </rule>

  <!-- 
    a >> à, en
  -->
  <rule weight="0.8">
    <match lemma="a" tags="pr"><select lemma="à"/></match>
  </rule>
  <rule weight="1.0">
    <match lemma="a" tags="pr"><select lemma="en"/></match>
    <or>
      <match lemma="Catalunya"/>
      <match lemma="Aragó"/>
      <match lemma="Castella"/>
      <match lemma="Andalusia"/>
      <match lemma="Occitània"/>
      <match lemma="Provença"/>
      <match lemma="Espanya"/>
      <match lemma="França"/>
      <match lemma="Alemanya"/>
      <match lemma="Suècia"/>
      <match lemma="Noruega"/>
      <match lemma="Finlàndia"/>
      <match lemma="Islàndia"/>
      <match lemma="Itàlia"/>
      <match lemma="Bèlgica"/>
      <match lemma="Holanda"/>
      <match lemma="Suïssa"/>
      <match lemma="Àustria"/>
      <match lemma="Hongria"/>
      <match lemma="Eslovàquia"/>
      <match lemma="Polònia"/>
      <match lemma="Lituània"/>
      <match lemma="Letònia"/>
      <match lemma="Estònia"/>
      <match lemma="Bielorússia"/>
      <match lemma="Grècia"/>
      <match lemma="Anglaterra"/>
      <match lemma="Gal·les"/>
      <match lemma="Irlanda"/>
      <match lemma="Rússia"/>
      <match lemma="Ucraïna"/>
      <match lemma="Bòsnia"/>
      <match lemma="Croàcia"/>
      <match lemma="Sèrbia"/>
      <match lemma="Xina"/>
      <match lemma="Índia"/>
      <match lemma="Turquia"/>
      <match lemma="Irak"/>
      <match lemma="Iraq"/>
      <match lemma="Iran"/>
      <match lemma="Aràbia"/>
      <match lemma="Síria"/>
      <match lemma="Jordània"/>
      <match lemma="Israel"/>
      <match lemma="Palestina"/>
      <match lemma="Austràlia"/>
      <match lemma="Àfrica"/>
      <match lemma="Amèrica"/>
      <match lemma="Àsia"/>
      <match lemma="Europa"/>
      <match lemma="Oceania"/>
      <match lemma="occident"/>
      <match lemma="orient"/>
    </or>
  </rule>

  <!-- 
    en >> en, dans, à
  -->
  <rule weight="0.8">
    <match lemma="en" tags="pr"><select lemma="dans"/></match>
  </rule>
  <rule weight="1.0">
    <or>
      <match lemma="classificar"/>
      <match lemma="consistir" tags="vblex.*"/>
      <match lemma="consistent" tags="adj.*"/>
      <match lemma="dividir" tags="vblex.*"/>
      <match lemma="dividit" tags="adj.*"/>
      <match lemma="organitzar" tags="vblex.*"/>
      <match lemma="organitzat" tags="adj.*"/>
    </or>
    <repeat from="0" upto="1">
      <match tags="adv"/>
    </repeat>
    <match lemma="en" tags="pr"><select lemma="en"/></match>
  </rule>
  <rule weight="1.2">
    <match lemma="classificar"/>
    <repeat from="0" upto="1">
      <match tags="adv"/>
    </repeat>
    <match tags="det.*.sg"/>
    <match lemma="en" tags="pr"><select lemma="dans"/></match>
  </rule>
  <rule weight="1.0">
    <match lemma="pensar"/>
    <repeat from="0" upto="1">
      <match tags="adv"/>
    </repeat>
    <match lemma="en" tags="pr"><select lemma="à"/></match>
  </rule>
  <rule weight="1.0">
    <match lemma="en" tags="pr"><select lemma="en"/></match>
    <match tags="*.inf"/>
  </rule>
  <rule weight="1.0">
    <match lemma="en" tags="pr"><select lemma="en"/></match>
    <match tags="n.*.sg"/>
  </rule>
  <rule weight="1.0">
    <match lemma="en" tags="pr"><select lemma="en"/></match>
    <match tags="adj.*"/>
  </rule>
  <rule weight="1.2">
    <match lemma="en" tags="pr"><select lemma="dans"/></match>
    <match tags="adj.ind.*"/>
  </rule>
  <rule weight="1.2">
    <match lemma="com"/>
    <match lemma="en" tags="pr"><select lemma="dans"/></match>
  </rule>

  <!-- 
    perquè (cnjadv) >> parce que, pour que
  -->
  <rule weight="0.8">
    <match lemma="perquè" tags="cnjadv"><select lemma="parce que"/></match>
  </rule>
  <rule weight="1.0">
    <match lemma="perquè" tags="cnjadv"><select lemma="pour que"/></match>
    <or>
      <match tags="*.prs.*"/>
      <match tags="*.pis.*"/>
    </or>
  </rule>
  <rule weight="1.0">
    <match lemma="perquè" tags="cnjadv"><select lemma="pour que"/></match>
    <match tags="prn.tn.*"/>
    <or>
      <match tags="*.prs.*"/>
      <match tags="*.pis.*"/>
    </or>
  </rule>

  <!-- 
    dues vegades DE cinc >> deux fois sur cinq
  -->
  <rule weight="1.0">
    <match tags="num.*"/>
    <match lemma="vegada" tags="n.*"/>
    <match lemma="de" tags="pr"><select lemma="sur" tags="pr"/></match>
    <match tags="num.*"/>
  </rule>
  <rule weight="0.8">
    <match lemma="de" tags="pr"><select lemma="de" tags="pr"/></match>
  </rule>

  <rule weight="0.8">
    <match lemma="pagar" tags="vblex.*"><select lemma="payer"/></match>
  </rule>
  <rule weight="0.8">
    <match lemma="canviar" tags="vblex.*"><select lemma="changer"/></match>
  </rule>

<!-- ADV -->

  <!-- 
    no >> ne, non
  -->
  <rule weight="0.8">
    <match lemma="no" tags="adv"><select lemma="ne"/></match>
  </rule>
  <rule weight="1.0">
    <match lemma="o" tags="cnjcoo"/>
    <match lemma="no" tags="adv"><select lemma="non"/></match>
    <or><match tags="cm"/><match tags="sent"/></or>
  </rule>
  <rule weight="1.0">
    <match lemma="no" tags="adv"><select lemma="non"/></match>
    <match tags="adj.*"/>
  </rule>

<!-- V -->

  <!-- congelar >> congeler, geler -->
  <rule weight="0.8">
    <match lemma="congelar" tags="vblex.*"><select lemma="congeler"/></match>
  </rule>
  <rule weight="1.0">
    <match lemma="congelar" tags="vblex.*"><select lemma="geler"/></match>
    <match tags="det.*"/>
    <match lemma="negociació" tags="n.*"/>
  </rule>

  <!-- passar >> passer, arriver -->
  <rule weight="0.8">
    <match lemma="passar" tags="vblex.*"><select lemma="passer"/></match>
  </rule>
  <rule weight="1.0">
    <match lemma="passar" tags="vblex.*"><select lemma="arriver"/></match>
    <match lemma="que"/>
  </rule>
  <rule weight="1.0">
    <match lemma="el"/>
    <match lemma="que" tags="rel.an.*"><select lemma="qui"/></match>
    <match lemma="passar" tags="vblex.*"><select lemma="arriver"/></match>
  </rule>

  <!-- succeir >> succéder, arriver -->
  <rule weight="0.8">
    <match lemma="succeir" tags="vblex.*"><select lemma="succéder"/></match>
  </rule>
  <rule weight="1.0">
    <match lemma="succeir" tags="vblex.*"><select lemma="arriver"/></match>
    <match lemma="que"/>
  </rule>
  <rule weight="1.0">
    <match lemma="el"/>
    <match lemma="que" tags="rel.an.*"><select lemma="qui"/></match>
    <match lemma="succeir" tags="vblex.*"><select lemma="arriver"/></match>
  </rule>

  <!-- intentar >> essayer, essayer# de -->
  <rule weight="0.8">
    <match lemma="intentar" tags="vblex.*"><select lemma="essayer"/></match>
  </rule>
  <rule weight="1.0">
    <match lemma="intentar" tags="vblex.*"><select lemma="essayer# de"/></match>
    <match tags="*.inf"/>
  </rule>
  <rule weight="1.0">
    <match lemma="intentar" tags="vblex.*"><select lemma="essayer# de"/></match>
    <match tags="adv"/>
    <match tags="*.inf"/>
  </rule>

  <!-- marejar >> faire# tourner la tête, sentir# mal -->
  <rule weight="0.8">
    <match lemma="marejar" tags="vblex.*"><select lemma="faire# tourner la tête"/></match>
  </rule>
  <rule weight="1.0">
    <match tags="prn.pro.ref.*"/>
    <match lemma="marejar" tags="vblex.*"><select lemma="sentir# mal"/></match>
  </rule>
  <rule weight="1.0">
    <match tags="prn.pro.p1.*"/>
    <match lemma="marejar" tags="vblex.*.p1.*"><select lemma="sentir# mal"/></match>
  </rule>
  <rule weight="1.0">
    <match tags="prn.pro.p2.*"/>
    <match lemma="marejar" tags="vblex.*.p2.*"><select lemma="sentir# mal"/></match>
  </rule>
  <rule weight="1.0">
    <match tags="prn.pro.ref.*"/>
    <match lemma="haver" tags="vbhaver.*"/>
    <match lemma="marejar" tags="vblex.pp.*"><select lemma="sentir# mal"/></match>
  </rule>
  <rule weight="1.0">
    <match tags="prn.pro.p1.*"/>
    <match lemma="haver" tags="vbhaver.*.p1.*"/>
    <match lemma="marejar" tags="vblex.pp.*"><select lemma="sentir# mal"/></match>
  </rule>
  <rule weight="1.0">
    <match tags="prn.pro.p2.*"/>
    <match lemma="haver" tags="vbhaver.*.p2.*"/>
    <match lemma="marejar" tags="vblex.pp.*"><select lemma="sentir# mal"/></match>
  </rule>
  <rule weight="1.0">
    <match tags="prn.pro.ref.*"/>
    <match lemma="anar" tags="vblex.pri.p3.*"/>
    <match lemma="marejar" tags="vblex.inf"><select lemma="sentir# mal"/></match>
  </rule>
  <rule weight="1.0">
    <match tags="prn.pro.p1.*"/>
    <match lemma="anar" tags="vblex.pri.p1.*"/>
    <match lemma="marejar" tags="vblex.inf"><select lemma="sentir# mal"/></match>
  </rule>
  <rule weight="1.0">
    <match tags="prn.pro.p2.*"/>
    <match lemma="anar" tags="vblex.pri.p2.*"/>
    <match lemma="marejar" tags="vblex.inf"><select lemma="sentir# mal"/></match>
  </rule>
  <rule weight="1.0">
    <match lemma="marejar" tags="vblex.*"><select lemma="sentir# mal"/></match>
    <match tags="prn.pro.ref.*"/>
  </rule>

  <!-- tractar >> traiter, tâcher, agir -->
  <rule weight="0.8">
    <match lemma="tractar" tags="vblex.*"><select lemma="traiter"/></match>
  </rule>
  <rule weight="1.0">
    <match lemma="tractar" tags="vblex.*"><select lemma="tâcher"/></match>
    <match lemma="de" tags="pr"/>
  </rule>
  <rule weight="1.0">
    <match lemma="tractar" tags="vblex.*"><select lemma="tâcher"/></match>
    <match tags="adv"/>
    <match lemma="de" tags="pr"/>
  </rule>
  <rule weight="1.2">
    <match lemma="tractar" tags="vblex.*"><select lemma="traiter"/></match>
    <match lemma="de" tags="pr"/>
    <match tags="det.def.*"/>
  </rule>
  <rule weight="1.2">
    <match lemma="tractar" tags="vblex.*"><select lemma="traiter"/></match>
    <match tags="adv"/>
    <match lemma="de" tags="pr"/>
    <match tags="det.def.*"/>
  </rule>
  <rule weight="3.2">
    <match tags="prn.pro.ref.*"/>
    <match lemma="tractar" tags="vblex.*"><select lemma="agir"/></match>
    <match lemma="de" tags="pr"/>
  </rule>
  <rule weight="3.2">
    <match tags="prn.pro.ref.*"/>
    <match lemma="tractar" tags="vblex.*"><select lemma="agir"/></match>
    <match tags="adv"/>
    <match lemma="de" tags="pr"/>
  </rule>
  <rule weight="3.2">
    <match lemma="tractar" tags="vblex.*"><select lemma="agir"/></match>
    <match tags="prn.pro.ref.*"/>
    <match lemma="de" tags="pr"/>
  </rule>

  <!-- cridar >> appeler, crier -->
  <rule weight="0.8">
    <match lemma="cridar" tags="vblex.*"><select lemma="appeler"/></match>
  </rule>
  <rule weight="1.0">
    <match lemma="cridar" tags="vblex.*"><select lemma="crier"/></match>
    <match lemma="a" tags="pr"/>
  </rule>
  <rule weight="1.0">
    <match lemma="cridar" tags="vblex.*"><select lemma="crier"/></match>
    <match tags="adv"/>
    <match lemma="a" tags="pr"/>
  </rule>
  <rule weight="1.0">
    <match lemma="li" tags="prn.pro.*"/>
    <match lemma="cridar" tags="vblex.*"><select lemma="crier"/></match>
  </rule>
  <rule weight="1.0">
    <match lemma="cridar" tags="vblex.*"><select lemma="crier"/></match>
    <match lemma="li" tags="prn.enc.*"/>
  </rule>

  <!-- 
    dir >> dire, appeler
  -->
  <rule weight="0.8">
    <match lemma="dir" tags="vblex.*"><select lemma="dire"/></match>
  </rule>
  <rule weight="1.0">
    <match tags="prn.pro.p1.*.sg"/>
    <match lemma="dir" tags="vblex.*.p1.sg"><select lemma="appeler"/></match>
  </rule>
  <rule weight="1.0">
    <match tags="prn.pro.p2.*.sg"/>
    <match lemma="dir" tags="vblex.*.p2.sg"><select lemma="appeler"/></match>
  </rule>
  <rule weight="1.0">
    <match tags="prn.pro.ref.p3.*.sp"/>
    <match lemma="dir" tags="vblex.*.p3.sg"><select lemma="appeler"/></match>
  </rule>
  <rule weight="1.0">
    <match tags="prn.pro.p1.*.pl"/>
    <match lemma="dir" tags="vblex.*.p1.pl"><select lemma="appeler"/></match>
  </rule>
  <rule weight="1.0">
    <match tags="prn.pro.p2.*.pl"/>
    <match lemma="dir" tags="vblex.*.p2.pl"><select lemma="appeler"/></match>
  </rule>
  <rule weight="1.0">
    <match tags="prn.pro.ref.p3.*.sp"/>
    <match lemma="dir" tags="vblex.*.p3.pl"><select lemma="appeler"/></match>
  </rule>
  <rule weight="1.2">
    <match lemma="dir" tags="vblex.*"><select lemma="dire"/></match>
    <match lemma="que" tags="cnjsub"/>
  </rule>
  <rule weight="1.2">
    <match lemma="dir" tags="vblex.*"><select lemma="dire"/></match>
    <match tags="adv"/>
    <match lemma="que" tags="cnjsub"/>
  </rule>


  <!-- il·lusionar >> donner# des espoirs, illusionner -->
  <rule weight="0.8">
    <match lemma="il·lusionar" tags="vblex.*"><select lemma="donner# des espoirs"/></match>
  </rule>
  <rule weight="1.0">
    <match tags="prn.pro.ref.*"/>
    <match lemma="il·lusionar" tags="vblex.*"><select lemma="illusionner"/></match>
  </rule>
  <rule weight="1.0">
    <match lemma="il·lusionar" tags="vblex.*"><select lemma="illusionner"/></match>
    <match tags="prn.enc.ref.*"/>
  </rule>

<!-- ADJ -->

  <!-- únic >> unique, seul -->
  <rule weight="0.8">
    <match lemma="únic" tags="adj.*"><select lemma="unique"/></match>
  </rule>
  <rule weight="1.0">
    <match lemma="únic" tags="adj.*"><select lemma="seul"/></match>
    <match tags="n.*"/>
  </rule>

  <!-- bàltic >> baltique, balte -->
  <rule weight="0.8">
    <match lemma="bàltic" tags="adj.*"><select lemma="baltique"/></match>
  </rule>
  <rule weight="1.0">
    <or>
      <match lemma="llengua" tags="n.*"/>
      <match lemma="idioma" tags="n.*"/>
      <match lemma="país" tags="n.*"/>
      <match lemma="nació" tags="n.*"/>
      <match lemma="estat" tags="n.*"/>
      <match lemma="govern" tags="n.*"/>
      <match lemma="parlament" tags="n.*"/>
      <match lemma="ciutadà" tags="n.*"/>
    </or>
    <match lemma="bàltic" tags="adj.*"><select lemma="balte"/></match>
  </rule>

  <!-- mig >> demi_post, demi_pre -->
  <rule weight="0.8">
    <match lemma="únic" tags="adj.*"><select lemma="demi_post"/></match>
  </rule>
  <rule weight="1.0">
    <match lemma="únic" tags="adj.*"><select lemma="demi_pre"/></match>
    <match tags="n.*"/>
  </rule>

<!-- N -->

  <!-- partit >> parti, match -->
  <rule weight="0.8">
    <match lemma="partit" tags="n.*"><select lemma="parti"/></match>
  </rule>
  <rule weight="1.0">
    <match lemma="partit" tags="n.*"><select lemma="match"/></match>
    <match lemma="de" tags="pr"/>
    <match tags="n.*.sg"/>
  </rule>

  <!-- serra >> scie, cordillère -->
  <rule weight="0.8">
    <match lemma="serra" tags="n.*"><select lemma="scie"/></match>
  </rule>
  <rule weight="1.0">
    <match lemma="serra" tags="n.*"><select lemma="cordillère"/></match>
    <match lemma="de" tags="pr"/>
    <or><match tags="np.*"/><match tags=""/></or>
  </rule>
  <rule weight="1.0">
    <match lemma="serra" tags="n.*"><select lemma="cordillère"/></match>
    <or>
      <match lemma="litoral" tags="adj.*"/>
      <match lemma="prelitoral" tags="adj.*"/>
      <match lemma="transversal" tags="adj.*"/>
    </or>
  </rule>

  <!-- coll >> cou, col -->
  <rule weight="0.8">
    <match lemma="coll" tags="n.*"><select lemma="cou"/></match>
  </rule>
  <rule weight="1.0">
    <match lemma="coll" tags="n.*"><select lemma="col"/></match>
    <match lemma="de" tags="pr"/>
  </rule>
  <rule weight="1.2">
    <match lemma="coll" tags="n.*"><select lemma="cou"/></match>
    <match lemma="de" tags="pr"/>
    <or><match tags="np.ant.*"/><match tags="np.cog.*"/></or>
