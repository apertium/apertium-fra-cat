      <match lemma="quart" tags="adj.*"/>
      <match lemma="cinquè" tags="adj.*"/>
      <match lemma="quint" tags="adj.*"/>
      <match lemma="sisè" tags="adj.*"/>
      <match lemma="setè" tags="adj.*"/>
      <match lemma="vuitè" tags="adj.*"/>
      <match lemma="novè" tags="adj.*"/>
      <match lemma="desè" tags="adj.*"/>
      <match lemma="penúltim" tags="adj.*"/>
      <match lemma="últim" tags="adj.*"/>
