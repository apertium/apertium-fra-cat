    </or>
    <repeat from="0" upto="1">
      <match tags="det.*"/>
    </repeat>
    <match lemma="principi" tags="n.*"><select lemma="début"/></match>
  </rule>
  <rule weight="1.0">
    <match lemma="principi" tags="n.*"><select lemma="début"/></match>
    <match lemma="de" tags="pr"/>
    <repeat from="0" upto="1">
