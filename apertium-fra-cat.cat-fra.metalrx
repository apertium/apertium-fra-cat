        <match tags="det.*"/>
        <match tags="adj.*"/>
      </or>
    </repeat>
    <or>
      <seq n="animals"/>
      <seq n="material"/>
    </or>
  </rule>
  <rule weight="1.0">
    <match lemma="cap" tags="n.*"><select lemma="tête"/></match>
    <match lemma="nuclear" tags="adj.*"/>
  </rule>
  <rule weight="1.0">
    <match lemma="cap" tags="n.*"><select lemma="tête"/></match>
    <repeat from="0" upto="4">
      <match tags="*"/>
    </repeat>
    <or>
      <seq n="animals"/>
      <seq n="part_cos"/>
      <match lemma="anatomia" tags="n.*"/>
    </or>
  </rule>
  <rule weight="1.0">
    <or>
      <seq n="animals"/>
      <seq n="part_cos"/>
      <match lemma="anatomia" tags="n.*"/>
    </or>
    <repeat from="0" upto="4">
      <match tags="*"/>
    </repeat>
    <match lemma="cap" tags="n.*"><select lemma="tête"/></match>
  </rule>

  <!-- capital >> capitale, chef-lieu -->
