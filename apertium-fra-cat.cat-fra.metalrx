      <match lemma="cantó" tags="n.*"/>
      <match lemma="regió" tags="n.*"/>
      <match lemma="departament" tags="n.*"/>
      <match lemma="província" tags="n.*"/>
      <match lemma="prefectura" tags="n.*"/>
      <match lemma="sots-prefectura" tags="n.*"/>
    </or>
  </rule>
  <rule weight="1.0">
    <match lemma="cap" tags="n.*"><select lemma="tête"/></match>
    <match lemma="de" tags="pr"/>
    <repeat from="0" upto="2">
